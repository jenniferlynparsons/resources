{
  "name": "resources",
  "version": "1.0.0",
  "description": "A collection of links",
  "main": "App.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js, jsx, css, json} \"",
    "test": "jest",
    "testu": "jest --silent -u",
    "testw": "jest --silent --watch",
    "testc": "jest --silent --coverage",
    "lint": "tslint --project .",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir dist --public-url ./",
    "buildgh": "parcel build src/index.html --out-dir docs --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenniferlynparsons/resources.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jennifer Lyn Parsons",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "bugs": {
    "url": "https://github.com/jenniferlynparsons/resources/issues"
  },
  "homepage": "https://github.com/jenniferlynparsons/resources#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "redux-devtools-extension": "^2.13.5",
    "sass": "^1.14.3"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "^23.3.9",
    "@types/react-redux": "^6.0.9",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/uuid": "^3.4.4",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "bulma": "^0.7.2",
    "identity-obj-proxy": "^3.0.0",
    "react": "^16.6.0",
    "react-click-outside": "github:tj/react-click-outside",
    "react-dom": "^16.5.2",
    "react-loadable": "^5.5.0",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
