{
  "name": "resources",
  "version": "1.0.0",
  "description": "A collection of links",
  "main": "App.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js, jsx, css, json} \"",
    "test": "jest",
    "testu": "jest --silent -u",
    "testw": "jest --silent --watch",
    "testc": "jest --silent --coverage",
    "lint": "tslint --project .",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --out-dir dist --public-url ./",
    "buildgh": "parcel build src/index.html --out-dir docs --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenniferlynparsons/resources.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Jennifer Lyn Parsons",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "bugs": {
    "url": "https://github.com/jenniferlynparsons/resources/issues"
  },
  "homepage": "https://github.com/jenniferlynparsons/resources#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.6.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.2.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.8",
    "sass": "^1.23.1"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "babel-eslint": "^10.0.3",
    "bulma": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "reach-router": "0.0.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
